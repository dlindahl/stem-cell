{
  "name": "stemcell",
  "version": "1.0.0",
  "description": "React build blocks focusing on typographic vertical rhythm",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "engines": {},
  "author": "Derek Lindahl <derek.lindahl@gmail.com>",
  "bugs": {
    "url": "https://github.com/dlindahl/stemcell/issues"
  },
  "contributors": [],
  "homepage": "http://dlindahl.github.io/stemcell",
  "keywords": [
    "blocks",
    "components",
    "react",
    "typography",
    "vertical rhythm"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlindahl/stemcell.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:commonjs": "npm run clean:commonjs && cross-env NODE_ENV=production cross-env BABEL_ENV=commonjs babel src --out-dir dist/commonjs --ignore *.stories.js,*.test.js",
    "build:es": "npm run clean:es && cross-env NODE_ENV=production cross-env BABEL_ENV=es babel src --out-dir dist/es --ignore *.stories.js,*.test.js",
    "clean:commonjs": "rimraf dist/commonjs",
    "clean:es": "rimraf dist/es",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "prettier-eslint --write \"./config/**/*.js\" \"./src/**/*.js\" \"./stories/**/*.js\" && eslint --ignore-path .gitignore --fix .",
    "start": "node scripts/start.js",
    "build-example": "node scripts/build.js",
    "pretest": "npm run lint",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs",
    "deploy-storybook": "storybook-to-ghpages",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "compass-vertical-rhythm": "^1.3.1",
    "dateformat": "^2.0.0",
    "fbjs": "^0.8.12",
    "html-parse-stringify": "^1.0.2",
    "lodash-es": "^4.17.4",
    "modularscale": "^2.0.0",
    "prop-types": "^15.5.9",
    "s-ago": "^1.1.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-deployer": "^1.2.0",
    "autoprefixer": "6.7.2",
    "babel-cli": "^6.24.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.2.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-plugin-transform-react-jsx-self": "^6.22.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "^4.0.0",
    "cross-spawn": "4.0.2",
    "detect-port": "1.1.0",
    "dotenv": "2.0.0",
    "eslint": "3.18.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-config-standard": "^7.1.0",
    "eslint-loader": "1.7.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "^2.1.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "fs-extra": "0.30.0",
    "glamor": "^2.20.24",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "prettier-eslint-cli": "^3.1.3",
    "promise": "7.1.1",
    "react": "^15.4.2",
    "react-dev-utils": "^0.5.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.2",
    "rimraf": "^2.6.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "peerDependencies": {
    "glamor": "^2.20.24",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/react.js",
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}
